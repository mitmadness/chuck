{
  "extends": "tslint:latest",
  "rules": {
    "curly": false,
    "quotemark": ["double"],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "comment-format": [],
    "triple-equals": false,
    "trailing-comma": [true, { "multiline": "never", "singleline": "never" }],
    "arrow-parens": false,
    "new-parens": false,
    "no-unused-expression": false
  }
}
